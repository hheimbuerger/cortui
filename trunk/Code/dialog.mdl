	SwitchImage(
		Multiply(IsNotEjected,Max(0,GetAmmo(Me,OnEveryFrame))),
		BlendImage(
			GroupImage([
				TranslateImage(
					ImportImage3D("gh_ammo_back", GHSideColor, true),
