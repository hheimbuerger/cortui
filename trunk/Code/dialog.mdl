				Subtract(1,Max(0,Multiply(5,Max(0,Subtract(1,Add(GetPercentHitPoints(Me,OnEveryFrame),0.8))))))
		),
	hudBlendMode);		
	
GHHullbottomright = 
	BlendImage(   
		GaugeImageRect(         
